{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "command": "confirm_delivery",
            "delivery_id": 3,
            "correctness": true,
            "order_id": 1,
            "color": "MAGENTA"
        }
    ],
    "required": [
        "command",
        "delivery_id",
        "correctness",
        "order_id",
        "color"
    ],
    "additionalProperties": true,
    "properties": {
        "command": {
            "$id": "#/properties/command",
            "type": "string",
            "title": "The command schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "confirm_delivery"
            ]
        },
        "delivery_id": {
            "$id": "#/properties/delivery_id",
            "type": "integer",
            "title": "The delivery_id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                3
            ]
        },
        "correctness": {
            "$id": "#/properties/correctness",
            "type": "boolean",
            "title": "The correctness schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                true
            ]
        },
        "order_id": {
            "$id": "#/properties/order_id",
            "type": "integer",
            "title": "The order_id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "color": {
            "$id": "#/properties/color",
            "type": "string",
            "title": "The color schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "MAGENTA"
            ]
        }
    }
}